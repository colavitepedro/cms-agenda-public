{
  "name": "cms-gestao-academica",
  "version": "1.0.0",
  "description": "Sistema completo de gestão acadêmica desenvolvido em React e Firebase",
  "main": "frontend/src/index.js",
  "scripts": {
    "dev": "cd frontend && npm start",
    "build": "cd frontend && npm run build",
    "start": "cd frontend && npm start",
    "install-frontend": "cd frontend && npm install",
    "install-functions": "cd functions && npm install",
    "install-all": "npm run install-frontend && npm run install-functions",
    "deploy": "npm run build && firebase deploy",
    "serve": "firebase emulators:start",
    "clean": "rimraf frontend/node_modules functions/node_modules frontend/build",
    "postinstall": "npm run install-frontend"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/colavitepedro/cms-gestao-academica.git"
  },
  "keywords": [
    "react",
    "firebase",
    "firestore",
    "vercel",
    "academia",
    "gestao",
    "calendario",
    "aulas",
    "disciplinas",
    "relatorios",
    "cms",
    "educacao"
  ],
  "author": {
    "name": "Pedro Colavite Conilho",
    "email": "colavitepedro1@gmail.com",
    "url": "https://github.com/colavitepedro"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/colavitepedro/cms-gestao-academica/issues"
  },
  "homepage": "https://github.com/colavitepedro/cms-gestao-academica#readme",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "firebase-tools": "^13.0.0",
    "rimraf": "^5.0.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@fullcalendar/daygrid": "^6.1.19",
    "@fullcalendar/interaction": "^6.1.19",
    "@fullcalendar/react": "^6.1.19",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "firebase": "^12.2.1",
    "firebase-admin": "^13.5.0",
    "firebase-functions": "^6.4.0",
    "jsonwebtoken": "^9.0.2",
    "react-router-dom": "^7.9.1",
    "styled-components": "^6.1.19",
    "web-vitals": "^5.1.0"
  }
}
