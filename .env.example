# Configurações do Backend
PORT=3001
NODE_ENV=development

# JWT Secret Key (substitua por uma chave forte em produção)
JWT_SECRET=sua_chave_secreta_jwt_aqui

# Firebase Configuration (Backend - Firebase Admin)
FIREBASE_PROJECT_ID=seu_projeto_firebase_id
GOOGLE_APPLICATION_CREDENTIALS=caminho/para/service-account-key.json

# Frontend - Firebase Configuration (Configure no Vercel Dashboard)
REACT_APP_FIREBASE_API_KEY=your_api_key_here
REACT_APP_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your_project_id
REACT_APP_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
REACT_APP_FIREBASE_APP_ID=your_app_id
REACT_APP_FIREBASE_MEASUREMENT_ID=your_measurement_id